<view><view class="place"></view><view class="user"><view class="left"><image src="{{user.imgUrl}}" data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" bindtap="__e"></image></view><view class="right"><view class="username">{{user.nickName}}</view></view><view class="erweima"><block wx:if="{{!hasLogin}}"><view data-event-opts="{{[['tap',[['bindLogin',['$event']]]]]}}" class="primary" bindtap="__e"><text class="login_text">登录</text></view></block><block wx:if="{{hasLogin}}"><view data-event-opts="{{[['tap',[['bindLogout',['$event']]]]]}}" class="default" bindtap="__e"><text class="login_text">退出</text></view></block></view></view><view class="order"><view class="list"><block wx:for="{{orderList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toOrderList',[index]]]]]}}" class="box" bindtap="__e"><view class="text">{{row.text}}</view></view></block></view><view class="lastMoney-info"><view class="left"><view class="box"><view class="num">{{user.lastMoney}}</view><view class="text">余额</view></view></view><view class="right"><view data-event-opts="{{[['tap',[['toDeposit']]]]}}" class="box" bindtap="__e"><view class="img"><view class="icon chongzhi"></view></view><view class="text">充值</view></view></view></view></view><view class="toolbar"><view class="title">我的工具栏</view><view class="list"><block wx:for="{{mytoolbarList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toPage',['$0'],[[['mytoolbarList','',index,'url']]]]]]]}}" class="box" bindtap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="text">{{row.text}}</view></view></block></view></view><view class="place-bottom"></view></view>